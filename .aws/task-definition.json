{
  "family": "chaima-task-def-pipeline",
  "containerDefinitions": [
    {
      "name": "pipeline-app",
      "image": "chaima22/backend-app",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256"
}
